<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uiucclapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ccl_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ccl_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.NoActionBar">

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/ccl_logo" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@style/Theme.Design.NoActionBar" />

        <activity android:name=".home" android:screenOrientation="sensorPortrait"  android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sign_up" android:screenOrientation="sensorPortrait" />
        <activity android:name=".log_In" android:screenOrientation="sensorPortrait"
            >

        </activity>



        <service
            android:name=".Services.PushNotification"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>